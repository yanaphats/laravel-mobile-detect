{
    "name": "sandbox-dev/laravel-mobile-detect",
    "description": "Laravel mobile detect package",
    "license": "MIT",
    "authors": [
        {
            "name": "Sandbox"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Riverskies\\Laravel\\MobileDetect\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sandbox\\MobileDetect\\MobileDetectServiceProvider"
            ],
            "aliases": {
                "MobileDetect": "Sandbox\\MobileDetect\\Facades\\MobileDetect"
            }
        }
    },
    "require": {
        "mobiledetect/mobiledetectlib": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "phpspec/prophecy-phpunit": "^1.1",
        "philo/laravel-blade": "^3.1",
        "laravel/laravel": "9.0.*"
    }
}
